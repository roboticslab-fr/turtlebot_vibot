<!--
  Used for visualising turtlebot in action with both rplidar and kinect running !
  
  It requires minimal_rplidar.launch and 3dsensor_rplidar.launch to already be up and running.
 -->
<launch>
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find turtlebot_vibot_bringup)/rviz/robot_rplidar_kinect.rviz" />
</launch>
